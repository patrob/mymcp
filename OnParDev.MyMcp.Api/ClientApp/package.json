{
  "name": "clientapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prestart": "node aspnetcore-https.cjs && node aspnetcore-react.cjs && npm run generate-api",
    "start": "rimraf ./dist && vite --port 5173",
    "predev": "node aspnetcore-https.cjs && node aspnetcore-react.cjs && npm run generate-api",
    "dev": "rimraf ./dist && vite --port 5173",
    "build": "npm run generate-api && tsc -b && vite build",
    "build:docker": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "generate-api": "npx openapi-typescript-codegen --input http://localhost:5099/openapi/v1.json --output ./src/api --client fetch"
  },
  "dependencies": {
