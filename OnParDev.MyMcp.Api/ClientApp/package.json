    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.4.2",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.4.49",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.3.5",
    "vitest": "^3.0.5"
